{
  "name": "ircapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "dev:local": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0",
    "start:local": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "irc:start": "tsx src/irc-server/start-server.ts",
    "irc:dev": "tsx watch src/irc-server/start-server.ts",
    "irc:check": "node check-network.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run irc:dev\"",
  "bot:start": "node webapp-bot.js",
  "dev:all": "concurrently \"npm run dev\" \"npm run irc:dev\" \"npm run bot:start\"",
    "setup:local": "node setup-network.js local",
    "setup:network": "node setup-network.js network",
    "oauth:setup": "node setup-oauth.js",
    "oauth:test": "node test-oauth.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@prisma/client": "^5.21.1",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "irc-framework": "^4.14.0",
    "jsonwebtoken": "^9.0.2",
    "next": "16.0.0",
    "next-auth": "^5.0.0-beta.29",
    "prisma": "^5.21.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tsx": "^4.20.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@railway/cli": "^4.11.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "prisma": "^5.21.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
